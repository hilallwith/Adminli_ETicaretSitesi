
@using Site.Identity
@model IEnumerable<ApplicationUser>

@{
    ViewBag.Title = "UserList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<section class="content">
<div class="row">
    <div class="col-xs-12">

        <!-- /.box -->
        <div class="box">
            <div class="box-header">
                <h3 class="box-title"> Kullanıcı Lstesi</h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body">

                <table id="example1" class="table table-bordered table-striped">
                    <thead>
                        <tr>
                            <th>Ad</th>

                            <th>Soyad</th>
                            <th>Kullanıxcı Ad</th>
                            <th>Mail </th>



                  


                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @item.Name
                                </td>
                                <td>
                                    @item.Surname
                                </td>
                                <td>
                                    @item.UserName
                                </td>
                                <td>
                                    @item.Email
                                </td>
                               


                                
                            </tr>
                        }


                    </tbody>
                    <tfoot>
                        <tr>
                            <th>Ad</th>

                            <th>Soyad</th>
                            <th>ullanıxcı Ad</th>
                            <th>Mail </th>



                            <th></th>
                        </tr>
                    </tfoot>
                </table>
            </div>
            <!-- /.box-body -->
        </div>
        <!-- /.box -->
    </div>
    <!-- /.col -->
</div>

</section>
